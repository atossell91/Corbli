@model LoginModel

@{
    ViewData["Title"] = "Login";
}

<h1>Register</h1>
<form asp-controller="Home" asp-action="RegisterSubmit" method="post">
    UserName: <input asp-for="UserName" /> <br />
    Password: <input asp-for="Password" /> <br />
    <button type="submit">Register</button>
</form>

